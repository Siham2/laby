<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    
    
    
    
    
    
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
        <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
        <div id="posX">
            <div class="{style.label}">Pos X</div>
            <div class="{style.value}" ui:field="posX"/>
        </div>
        <div id="posY">
            <div class="{style.label}">Pos Y</div>
            <div class="{style.value}" ui:field="posY"/>
        </div>
        <div id="imageX">
            <div class="{style.label}">Image X</div>
            <div class="{style.value}" ui:field="imageX"/>
        </div>
        <div id="imageY">
            <div class="{style.label}">Image Y</div>
            <div class="{style.value}" ui:field="imageY"/>
        </div>
        <div id="type">
            <div class="{style.label}">Type</div>
            <div class="{style.value}" ui:field="type"/>
        </div>
        <div id="niveau">
            <div class="{style.label}">Niveau</div>
            <div class="{style.value}" ui:field="niveau"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
