<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:org.laby.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:org.laby.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  <div id="imageX">
            <div class="{style.label}">Image X</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="imageX"/>
            </div>
        </div>
        <div id="imageY">
            <div class="{style.label}">Image Y</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="imageY"/>
            </div>
        </div>
        <div id="posY">
            <div class="{style.label}">Pos Y</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="posY"/>
            </div>
        </div>
        <div id="posX">
            <div class="{style.label}">Pos X</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="posX"/>
            </div>
        </div>
        <div id="type">
            <div class="{style.label}">Type</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="type"/>
            </div>
        </div>
        <div id="niveau">
            <div class="{style.label}">Niveau</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="niveau"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
