<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:org.laby.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:org.laby.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    

    <g:Button ui:field="save">Save</g:Button>
  
        
        
        
        
    <div id="nom">
            <div class="{style.label}">Nom</div>
            <div class="{style.value}">
                <g:TextBox ui:field="nom"/>
            </div>
        </div>
        <div id="canvaCouloir">
            <div class="{style.label}">Canva Couloir</div>
            <div class="{style.value}">
                <g:TextBox ui:field="canvaCouloir"/>
            </div>
        </div>
        <div id="canvaMur">
            <div class="{style.label}">Canva Mur</div>
            <div class="{style.value}">
                <g:TextBox ui:field="canvaMur"/>
            </div>
        </div>
        <div id="canvaBonus">
            <div class="{style.label}">Canva Bonus</div>
            <div class="{style.value}">
                <g:TextBox ui:field="canvaBonus"/>
            </div>
        </div>
        <div id="canvaSortie">
            <div class="{style.label}">Canva Sortie</div>
            <div class="{style.value}">
                <g:TextBox ui:field="canvaSortie"/>
            </div>
        </div>
        <div id="canvaMechants">
            <div class="{style.label}">Canva Mechants</div>
            <div class="{style.value}">
                <g:TextBox ui:field="canvaMechants"/>
            </div>
        </div>
        <div id="tailleX">
            <div class="{style.label}">Taille X</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="tailleX"/>
            </div>
        </div>
        <div id="tailleY">
            <div class="{style.label}">Taille Y</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="tailleY"/>
            </div>
        </div>
   
    </g:HTMLPanel>
</ui:UiBinder>
